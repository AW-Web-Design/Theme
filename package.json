{
  "version": "0.1.0-alpha-2",
  "license": "MIT",
  "bin": "dist/cli.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && yarn copy",
    "copy": "cp ./src/cli.js ./dist/cli.js && cp ./src/config.json ./dist/config.json && cp -r ./src/templates ./dist && cp -r ./src/theme ./dist && cp ./orchard.theme.config.json ./dist",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@aw-web-design/theme",
  "author": "The-Code-Monkey",
  "module": "dist/theme.esm.js",
  "size-limit": [
    {
      "path": "dist/theme.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/theme.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "@types/sade": "^1.7.2",
    "husky": "^6.0.0",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@aw-web-design/styled-system": "^0.1.0",
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.170",
    "@types/lodash-es": "^4.17.4",
    "@types/tinycolor2": "^1.4.2",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "sade": "^1.7.4",
    "style-dictionary": "^2.10.3",
    "tinycolor2": "^1.4.2"
  }
}
